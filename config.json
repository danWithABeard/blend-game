{
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "brace-style": [2, "1tbs"],
    "quotes": [2, "single", "avoid-escape"],
    "keyword-spacing": 2,
    "space-unary-ops": 2,
    "curly": 0,
    "eqeqeq": [2, "smart"],
    "no-floating-decimal": 2,
    "no-comma-dangle": 0,
    "no-use-before-define": [2, "nofunc"],
    "strict": 0
  }
}
